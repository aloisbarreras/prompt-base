{
  "name": "prompt-base",
  "description": "Base prompt. Works as a standalone prompt, or with Enquirer as a plugin. Only two tiny dependencies.",
  "version": "0.0.0",
  "homepage": "https://github.com/enquirer/prompt-base",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-base",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-base/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "mocha",
    "cover": "nyc --reporter=text --reporter=html mocha"
  },
  "dependencies": {
    "ansi-colors": "^2.0.1",
    "data-store": "^3.0.1"
  },
  "devDependencies": {
    "yosay": "^2.0.2",
    "mocha": "^5.2.0",
    "gulp-format-md": "^1.0.0",
    "nyc": "^12.0.2"
  },
  "keywords": [
    "base",
    "answer",
    "answers",
    "ask",
    "cli",
    "command",
    "command-line",
    "enquirer",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interface",
    "menu",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "base"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "prompt-checkbox",
        "prompt-radio"
      ]
    },
    "reflinks": [
      "enquirer"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
